type User {
    id: ID!
    createdAt: String!
    updatedAt: String!
    username: String!
    password: String!
}

type Query {
    getAllUsers: [User!]!
    getOneUser(userId: ID!): User!
}

type Mutation {
    createUser(username: String!, password: String!): User!
    updateOneUser(userId: ID!, username: String!, password: String!): User!
    deleteOneUser(userId: ID!): Boolean!
}
